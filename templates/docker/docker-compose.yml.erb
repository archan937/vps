version: "3"

services:

  <%- services.each do |service| %>
  <%= service.name %>:
    image: <%= service.image %>
    container_name: <%= service.name %>

  <%- end %>
  <%- upstreams.each do |upstream| %>
  <%= upstream.name %>:
    image: <%= upstream.name %>
    container_name: <%= upstream.name %>

  <%- end %>
